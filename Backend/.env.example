# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here

# DynamoDB Tables
DYNAMODB_TABLE_NAME=your_table_name_here

# AWS Cognito
COGNITO_USER_POOL_ID=your_user_pool_id_here
COGNITO_CLIENT_ID=your_client_id_here

# API Keys for AI Services
GROQ_API_KEY=your_groq_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Application Settings
APP_NAME="RU Carpooling Backend"
LOG_LEVEL=INFO
DEBUG=false

# JWT Configuration (use a secure random key in production)
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=30

# S3 Configuration (for avatar storage)
S3_BUCKET_NAME=your_s3_bucket_name_here
